# WayfinDex Configuration Example
# Copy this file to config.yaml and update with your actual settings

# List of active agents to use
# Options: gemini, openai, perplexity, openrouter, anthropic
active_agents:
  - openai

# OpenAI Configuration
openai_api_key_env_var: OPENAI_API_KEY
openai_model_names:
  - gpt-4
  - gpt-3.5-turbo

# Gemini Configuration (optional)
# gemini_api_key_env_var: GEMINI_API_KEY
# gemini_model_names:
#   - gemini-pro

# Perplexity Configuration (optional)
# perplexity_api_key_env_var: PERPLEXITY_API_KEY
# perplexity_model_names:
#   - pplx-7b-online

# OpenRouter Configuration (optional)
# openrouter_api_key_env_var: OPENROUTER_API_KEY
# openrouter_model_names:
#   - anthropic/claude-2

# Anthropic Configuration (optional)
# anthropic_api_key_env_var: ANTHROPIC_API_KEY
# anthropic_model_names:
#   - claude-3-opus-20240229
